            "items": [ "Self", "Ada", "Apples", "Crab Nebula", "Earth", "Everest", "Flower", "Forest", "Hong Kong", "Lighthouse", "Sunrise" ],
            "default_choice": 0,
            "choice": { "functions": [ "targ_image_menu_generator" ] }
        },
        {
            "name": "rule_menu_generator",
